  "bugs": {
    "url": "https://github.com/Rolandkuku/game-on/issues"
  },
  "homepage": "https://github.com/Rolandkuku/game-on#readme",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "ejs": "^2.5.2",
    "mocha": "^3.0.2",
    "nodemon": "^1.10.2",
    "react": "^15.3.2",
    "redux": "^3.6.0"
  },
  "dependencies": {
    "express": "^4.14.0"

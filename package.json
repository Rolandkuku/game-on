    "babel-core": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.13.0",
    "babel-register": "^6.14.0",
    "bootstrap": "^4.0.0-alpha.5",
